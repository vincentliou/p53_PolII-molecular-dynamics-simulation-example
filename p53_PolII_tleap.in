source leaprc.protein.ff14SB
source leaprc.DNA.OL15
source leaprc.gaff2
source leaprc.water.tip3p
mol = loadpdb final.pdb
savepdb mol p53_polII_dry2.pdb
saveamberparm mol p53_polII_dry2.prmtop p53_polII_dry2.inpcrd
solvateBox mol TIP3PBOX 12.0
charge mol
addions mol Na+ 0
addions mol Na+ 535
addions mol Cl- 0
savepdb mol p53_polII_solv2.pdb
saveAmberParm mol p53_polII2.prmtop p53_polII2.inpcrd
quit
 
